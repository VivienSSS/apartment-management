package web

templ LogInPageMetadata() {
	<title>Login Page</title>
}

templ LogInPage() {
	@BaseHTML(BaseHTMLProps{
		Metadata: LogInPageMetadata(),
	}) {
		<main>
			<form>
				<div>
					<label>Email</label>
					<input type="email" name="email"/>
				</div>
				<div>
					<label>Password</label>
					<input type="password" name="password"/>
				</div>
				<button type="submit">
					Sign In
				</button>
			</form>
		</main>
	}
}
